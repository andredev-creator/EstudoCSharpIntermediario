<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings> <!--Tem que estar aqui dentro por são os ajustes da nossa aplicação-->
    <add key="provider" value="MSSQL"/> <!--MSSQL = Sql Server, MYSQL = MySQL-->
    <add key="server" value=".\SQLEXPRESS"/>
    <add key="database" value="CSharpIntermediario" />
    <add key="user" value="sa"/>
    <add key="password" value="123456"/>  
  </appSettings>
  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.8.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
</configuration>